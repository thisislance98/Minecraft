    "nanoid": "^3.3.11",
    "peerjs": "^1.5.5",
    "puppeteer": "^24.34.0",
    "simple-peer": "^9.11.1",
    "simplex-noise": "^4.0.3",
    "socket.io-client": "^4.8.3",
    "stripe": "^20.1.0",
    "three": "^0.160.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.2.3",
    "three-perf": "^1.0.11",
